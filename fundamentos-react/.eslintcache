in = 50\n    const max = 70\n    const gerarIdade = () => parseInt(Math.random() * (max - min)) + min\n    const gerarNerd = () => Math.random() > 0.5\n\n    return (\n        <div>\n            <div>Filho</div>\n            <button onClick={_ => cb('Jo√£o', gerarIdade(), gerarNerd())}>\n                Fornecer Informacoes\n            </button>\n        </div>\n    )\n}\n","/home/ingressolive/Documentos/curso-react/fundamentos-react/src/Components/comunicacao/IndiretaPai.jsx",["134"],"import React, { useState } from 'react'\nimport IndiretaFilho from './IndiretaFiho'\n\nexport default props => {\n\n    const [nome, setNome] = useState('?')\n    const [idade, setIdade] = useState(0)\n    const [nerd, setNerd] = useState(false)\n\n    function fornecerInformacoes(nome, idade, nerd) {\n        setNome(nome)\n        setIdade(idade)\n        setNerd(nerd)\n    }\n\n    return (\n        <div>\n            <div>\n            <span>{nome} </span>\n            <span><strong>{idade}</strong> </span>\n            <span>{nerd ? 'Verdadeiro' : 'Falso'}</span>\n            </div>\n            <IndiretaFilho quandoClicar={fornecerInformacoes}></IndiretaFilho>\n        </div>\n    )\n}","/home/ingressolive/Documentos/curso-react/fundamentos-react/src/Components/formulario/Input.jsx",["135"],"import React, { useState } from 'react'\nimport \"./Input.css\"\n\nexport default props => {\n\n    const [valor, setValor] = useState('Inicial')\n\n    function quandoMudar(e) {\n        setValor(e.target.value)\n    }\n\n    return (\n        <div className=\"Input\">\n            <h2>{valor}</h2>\n            <div style={{\n                display: 'flex',\n                flexDirection: 'column'\n            }}>\n                <input value={valor} onChange={quandoMudar} />\n                <input value={valor} readOnly />\n                <input value={undefined} />\n            </div>\n        </div>\n    )\n}","/home/ingressolive/Documentos/curso-react/fundamentos-react/src/Componen